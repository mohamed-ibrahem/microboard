{
    "name": "devnile/microboard-v2",
    "description": "This package creates to decrease working time that spent on admin panels",
    "keywords": [
        "devnile",
        "microboard-v2"
    ],
    "homepage": "https://github.com/devnile/microboard-v2",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Ibrahem",
            "email": "mohamed.ibrahim@devnile.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Devnile\\MicroboardV2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devnile\\MicroboardV2\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devnile\\MicroboardV2\\MicroboardV2ServiceProvider"
            ],
            "aliases": {
                "MicroboardV2": "Devnile\\MicroboardV2\\MicroboardV2Facade"
            }
        }
    }
}
