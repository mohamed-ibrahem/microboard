{
    "name": "mohamed-ibrahem/microboard",
    "description": "This package creates to decrease working time that spent on admin panels",
    "homepage": "https://github.com/devnile/microboard-v2",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Ibrahem",
            "email": "mohamed.ibrahim@devnile.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Microboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Microboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microboard\\Providers\\MicroboardCoreServiceProvider"
            ],
            "aliases": {
                "Microboard": "Microboard\\Microboard"
            }
        }
    }
}
