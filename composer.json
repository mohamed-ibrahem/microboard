{
    "name": "mohamed-ibrahem/microboard",
    "description": "This package creates to decrease working time that spent on admin panels",
    "homepage": "https://github.com/devnile/microboard-v2",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Ibrahem",
            "email": "mohamed.ibrahim@devnile.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0",
        "livewire/livewire": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0",
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Microboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Microboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microboard\\Providers\\MicroboardCoreServiceProvider"
            ]
        }
    }
}
